{
  "name": "vscode-vector-aquarium",
  "displayName": "vscode-vector-aquarium",
  "description": "Make your vscode an aquarium and heal you !",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-vector-aquarium.open",
    "onView:vscode-vector-aquarium.view"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vscode-vector-aquarium",
          "title": "Vector Aquarium",
          "icon": "assets/icon.svg"
        }
      ]
    },
    "views": {
      "vscode-vector-aquarium": [
        {
          "type": "webview",
          "id": "vscode-vector-aquarium.view",
          "name": "Vector Aquarium"
        }
      ]
    },
    "commands": [
      {
        "command": "vscode-vector-aquarium.open",
        "title": "Vector Aquarium"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.69.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}